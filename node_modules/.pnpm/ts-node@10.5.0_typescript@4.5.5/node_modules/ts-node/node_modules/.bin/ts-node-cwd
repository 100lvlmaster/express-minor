#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/dist/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/node_modules:/Users/apple/Docs/node/todo-service/node_modules:/Users/apple/Docs/node/node_modules:/Users/apple/Docs/node_modules:/Users/apple/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/dist/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/node_modules:/Users/apple/Docs/node/todo-service/node_modules:/Users/apple/Docs/node/node_modules:/Users/apple/Docs/node_modules:/Users/apple/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../dist/bin-cwd.js" "$@"
fi
