#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/node_modules:/Users/apple/Docs/node/todo-service/node_modules:/Users/apple/Docs/node/node_modules:/Users/apple/Docs/node_modules:/Users/apple/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/nopt@1.0.10/node_modules:/Users/apple/Docs/node/todo-service/node_modules/.pnpm/node_modules:/Users/apple/Docs/node/todo-service/node_modules:/Users/apple/Docs/node/node_modules:/Users/apple/Docs/node_modules:/Users/apple/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nopt.js" "$@"
else
  exec node  "$basedir/../../bin/nopt.js" "$@"
fi
